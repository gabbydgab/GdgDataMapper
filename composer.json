{
    "name": "gabbydgab/gdg-data-mapper",
    "description": "Abstract PHP library that moves data between objects and a database while keeping them independent of each other and the mapper itself",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "gabbydgab",
        "data mapper"
    ],
    "homepage": "https://github.com/gabbydgab/GdgDataMapper",
    "authors": [
        {
            "name": "Gab Amba",
            "email": "gamba@gabbydgab.com",
            "homepage": "http://www.gabbydgab.com/"
        }
    ],
    "require": {
        "zendframework/zend-db": "2.3.*",
        "zendframework/zend-stdlib": "2.3.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "phpunit/php-invoker": "1.*"
    },
    "autoload": {
        "psr-4": {
            "GdgDataMapper\\Entity\\": "src/GdgDataMapper/Entity/",
            "GdgDataMapper\\Mapper\\": "src/GdgDataMapper/Mapper/"
        },
        "classmap": [
            "./src/GdgDataMapper/AbstractEntityTrait.php",
            "./src/GdgDataMapper/AbstractMapperTrait.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "GdgDataMapperTest\\": "tests/GdgDataMapperTest/"
        }
    }
}